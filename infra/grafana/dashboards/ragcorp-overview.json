    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "id": 3,
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"ragcorp-backend\", status=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"ragcorp-backend\"}[5m])) * 100",
